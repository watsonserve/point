$primaryColor = #4dbf9f
$lightPrimaryColor = #eefafb
$primaryColorAlpha = rgba(24, 186, 226, .15)
$primaryShadowColor = rgba(16, 132, 160, .24)
$primaryHoverShadowColor = rgba(16, 132, 160, .33)
$backColor = #eff4f6
$fontColor = #333
$grayFontColor = #787c83
$lightGrayColor = #f9f9f9
$grayColor = #adb8bb
$grayHoverColor = #f5f5f5
$borderColor = #dee1e4
$tableBorderColor = rgba(222, 225, 228, .3)
$inputShadowColor = rgba(141, 155, 165, .1)
$inputHoverShadowColor = rgba(141, 155, 165, .2)
$errorRed = #ff6698
$warningColor = #ffac38
$linkColor = #3c95ef
$lineColor = #eee
$bgWhite = #fff
$headerHeight = 64px

@keyframes rotateUp
  from
    transform rotateX(0)

  to
    transform rotateX(180deg)

@keyframes rotateDown
  from
    transform rotateX(-180deg)

  to
    transform rotateX(0)
